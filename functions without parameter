def display():
    n=int(input("enter the number to display"))
    i=1
    while i<=10:
        print(i)
        i=i+1
def Sums():
    n=int(input("enter the number to find sum"))
    Sum=0
    i=1
    while i<=n:
        Sum=Sum+i
        i=i+1
    print(Sum)
display()
Sums()
        
    
    
    
 
    
   
    
   
